{
  "name": "model",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "src/model.js",
  "module": "src/model.js",
  "description": "generic meta model",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "pretest": "rollup -c tests/rollup.config.js",
    "test": "ava --timeout 2m",
    "posttest": "npm run prepare && markdown-doctest",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme src/model.js --section=API",
    "lint": "documentation lint src/model.js"
  },
  "dependencies": {
    "registry-mixin": "3.0.2"
  },
  "devDependencies": {
    "ava": "^3.6.0",
    "c8": "^7.1.0",
    "documentation": "^12.2.0",
    "rollup": "^1.32.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-executable": "^1.5.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "semantic-release": "^17.0.4"
  },
  "engines": {
    "node": ">=12.16.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/model.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/model/issues"
  },
  "homepage": "https://github.com/arlac77/model#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-github"
    ]
  }
}
