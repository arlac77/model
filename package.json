{
  "name": "model",
  "version": "0.0.0-semantic-release",
  "description": "generic meta model",
  "keywords": [],
  "main": "dist/Model.js",
  "module": "src/Model.js",
  "scripts": {
    "cover": "npm run build && node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports tests/*_test.js",
    "test": "npm run build && node_modules/.bin/mocha tests/*_test.js && markdown-doctest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js > README.md",
    "build": "node_modules/rollup/bin/rollup --output=dist/Model.js -c -- src/Model.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/model.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/model/issues"
  },
  "homepage": "https://github.com/arlac77/model#readme",
  "dependencies": {
    "registry-mixin": "^2.1.7"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "rollup": "^0.38.0",
    "semantic-release": "^6.3.5",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.1"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.9.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}