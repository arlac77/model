{
  "name": "model",
  "version": "0.0.0-semantic-release",
  "description": "generic meta model",
  "keywords": [],
  "main": "dist/model.js",
  "module": "src/model.js",
  "scripts": {
    "cover": "nyc npm test",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "pretest": "rollup -c",
    "posttest": "markdown-doctest",
    "precover": "rollup -c",
    "prepare": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/model.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/model/issues"
  },
  "homepage": "https://github.com/arlac77/model#readme",
  "dependencies": {
    "registry-mixin": "2.1.14"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^3.5.2",
    "rollup": "^0.49.3",
    "semantic-release": "^8.0.0",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^11.2.1"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.4.0"
  },
  "nyc": {
    "include": ["dist/**/*.js"],
    "reporter": ["lcov"],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  }
}
