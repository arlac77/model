{
  "name": "model",
  "version": "0.0.0-semantic-release",
  "description": "generic meta model",
  "keywords": [],
  "main": "dist/Model.js",
  "module": "src/Model.js",
  "scripts": {
    "cover": "nyc npm test",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js >README.md",
    "prepublish": "rollup --output=dist/Model.js -c -- src/Model.js",
    "pretest": "npm run prepublish",
    "posttest": "markdown-doctest",
    "precover": "npm run prepublish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/model.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/model/issues"
  },
  "homepage": "https://github.com/arlac77/model#readme",
  "dependencies": {
    "registry-mixin": "2.1.13"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^2.0.0",
    "mocha": "^3.3.0",
    "rollup": "^0.41.6",
    "semantic-release": "^6.3.6",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "nyc": "^10.3.2"
  },
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=7.9.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "nyc": {
    "include": [
      "dist/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/Kronos-Tools/npm-package-template.git"
    }
  }
}