{
  "name": "model",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./src/model.js",
  "exports": {
    ".": "./src/model.js"
  },
  "description": "generic meta model",
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme ./src/model.js --section=API",
    "lint": "documentation lint ./src/model.js"
  },
  "dependencies": {
    "registry-mixin": "3.0.3"
  },
  "devDependencies": {
    "ava": "^3.13.0",
    "c8": "^7.3.1",
    "documentation": "^13.0.2",
    "semantic-release": "^17.1.2"
  },
  "engines": {
    "node": ">=12.16.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/model.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/model/issues"
  },
  "homepage": "https://github.com/arlac77/model#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-esm-only",
      "arlac77/template-github"
    ]
  }
}
