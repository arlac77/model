{
  "name": "model",
  "version": "0.0.0-semantic-release",
  "description": "generic meta model",
  "keywords": [],
  "main": "lib/model.js",
  "scripts": {
    "cover": "npm run build && ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests/*_test.js",
    "test": "npm run build && mocha tests/*_test.js && markdown-doctest",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f {{module}} > README.md",
    "build": "./node_modules/rollup/bin/rollup --format=cjs --output=lib/model.js -- {{module}}"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/model.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/model/issues"
  },
  "homepage": "https://github.com/arlac77/model#readme",
  "dependencies": {
    "registry-mixin": "^2.1.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "semantic-release": "^6.3.1",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.0"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.9.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}